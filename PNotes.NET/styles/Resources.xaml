    <SolidColorBrush x:Key="TitleBarButtonBorderBrush" Color="Transparent"/>
    <SolidColorBrush x:Key="TitleBarButtonBackgroundBrush" Color="Transparent"/>
    <sys:Double x:Key="TitleBarMinButtonWidth">34</sys:Double>
    <sys:Double x:Key="TitleBarMinButtonHeight">26</sys:Double>
    <Style x:Key="TitleBarMinButtonBorder" TargetType="{x:Type Border}">
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Background" Value="{DynamicResource TitleBarButtonBackgroundBrush}"/>
    </Style>
    <Style x:Key="TitleBarMinButtonPath" TargetType="{x:Type Path}">
        <Setter Property="Stroke" Value="{Binding Foreground, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type Button}}}"/>
        <Setter Property="Fill" Value="Transparent"/>
        <Setter Property="StrokeThickness" Value="1"/>
        <Setter Property="Stretch" Value="None"/>
        <Setter Property="RenderOptions.EdgeMode" Value="Aliased"/>
        <Setter Property="Data" Value="M0,8 H8 M0,7 H8 M0,6 H8"/>
    </Style>
    <sys:Double x:Key="TitleBarMaxButtonWidth">34</sys:Double>
    <sys:Double x:Key="TitleBarMaxButtonHeight">26</sys:Double>
    <Style x:Key="TitleBarMaxButtonBorder" TargetType="{x:Type Border}">
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Background" Value="{DynamicResource TitleBarButtonBackgroundBrush}"/>
    </Style>
    <Style x:Key="TitleBarMaxButtonPath1" TargetType="{x:Type Path}">
        <Setter Property="Stroke" Value="{Binding Foreground, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type Button}}}"/>
        <Setter Property="StrokeThickness" Value="1"/>
        <Setter Property="Stretch" Value="None"/>
        <Setter Property="RenderOptions.EdgeMode" Value="Aliased"/>
        <Setter Property="Data" Value="M0,0 H8 V8 H0 V0 M0,1 H8 M0,2 H8"/>
    </Style>
    <Style x:Key="TitleBarMaxButtonPath2" TargetType="{x:Type Path}">
        <Setter Property="Stroke" Value="{Binding Foreground, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type Button}}}"/>
        <Setter Property="StrokeThickness" Value="1"/>
        <Setter Property="Stretch" Value="None"/>
        <Setter Property="RenderOptions.EdgeMode" Value="Aliased"/>
        <Setter Property="Data" Value="M0,3 H5 V8 H0 V3 M0,4 H5 M3,0 H8 V5 H7 M3,1 H8"/>
    </Style>
    <sys:Double x:Key="TitleBarCloseButtonWidth">34</sys:Double>
    <sys:Double x:Key="TitleBarCloseButtonHeight">26</sys:Double>
    <Style x:Key="TitleBarCloseButtonBorder" TargetType="{x:Type Border}">
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Background" Value="{DynamicResource TitleBarButtonBackgroundBrush}"/>
    </Style>
    <Style x:Key="TitleBarCloseButtonPath1" TargetType="{x:Type Path}">
        <Setter Property="Stroke" Value="{Binding Foreground, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type Button}}}"/>
        <Setter Property="StrokeThickness" Value="1.5"/>
        <Setter Property="Stretch" Value="None"/>
        <Setter Property="RenderOptions.EdgeMode" Value="Aliased"/>
        <Setter Property="Data" Value="M0,0 L8,8"/>
    </Style>
    <Style x:Key="TitleBarCloseButtonPath2" TargetType="{x:Type Path}">
        <Setter Property="Stroke" Value="{Binding Foreground, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type Button}}}"/>
        <Setter Property="StrokeThickness" Value="1.5"/>
        <Setter Property="Stretch" Value="None"/>
        <Setter Property="RenderOptions.EdgeMode" Value="Aliased"/>
        <Setter Property="Data" Value="M0,8 L8,0"/>
    </Style>
    
